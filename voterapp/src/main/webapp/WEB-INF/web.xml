<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>voterapp</display-name>
  <welcome-file-list>
    <welcome-file>register.html</welcome-file>
  </welcome-file-list>
  <servlet>
      <servlet-name>abc</servlet-name>
      <servlet-class>com.carrerit.voterapp.servlets.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>abc</servlet-name>
     <url-pattern>/register</url-pattern>
  </servlet-mapping>
   <servlet>
      <servlet-name>xyz</servlet-name>
      <servlet-class>com.carrerit.voterapp.servlets.DisplayServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>xyz</servlet-name>
     <url-pattern>/displayvoter</url-pattern>
  </servlet-mapping>
   <servlet>
      <servlet-name>delete</servlet-name>
      <servlet-class>com.carrerit.voterapp.servlets.DeleteVoter</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>delete</servlet-name>
     <url-pattern>/deleteuser</url-pattern>
  </servlet-mapping>
   <servlet>
      <servlet-name>select</servlet-name>
      <servlet-class>com.carrerit.voterapp.servlets.SelectVoter</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>select</servlet-name>
     <url-pattern>/selectvoter</url-pattern>
  </servlet-mapping>
</web-app>